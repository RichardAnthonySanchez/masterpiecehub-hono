{
  "name": "masterpiecehub-hono",
  "type": "module",
  "scripts": {
    "seed": "node --env-file=.env scripts/seed.ts",
    "dev": "tsx watch --env-file=.env src/index.tsx",
    "build:css": "npx tailwindcss -i ./src/static/styles/styles.css -o ./src/static/styles/output.css --minify",
    "build": "tsc && npm run build:css",
    "start": "node dist/src/index.js"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.6",
    "@typegoose/typegoose": "^12.20.0",
    "csvtojson": "^2.0.14",
    "hono": "^4.10.4",
    "mongodb": "^6.20.0",
    "mongoose": "^8.19.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "autoprefixer": "^10.4.21",
    "daisyui": "^4.12.24",
    "postcss": "^8.5.6",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3"
  }
}
